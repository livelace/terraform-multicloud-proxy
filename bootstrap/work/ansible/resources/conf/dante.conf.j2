external: eth0
external.rotation: route

internal: 0.0.0.0 port = {{ socks_port }}

logoutput: stderr

method: username none
clientmethod: none

{% for item in whitelist %}
client pass {
  from: {{ item }} port 1-65535 to: 0.0.0.0/0
}
{% endfor %}

pass {
  from: 0.0.0.0/0 to: 0.0.0.0/0
  protocol: tcp udp
}
